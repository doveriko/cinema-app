{
    "version": 2,
    "builds": [
        {
        "src": "./server/app.js",
        "use": "@vercel/node"
        },
        {
        "src": "./client/build",
        "use": "@vercel/static"
        }
    ],
    "routes": [
        {
        "src": "/client/(.*)",
        "dest": "/src/routes.js"
        },
        {
        "src": "/server/(.*)",
        "dest": "/app.js"
        }
    ]
}