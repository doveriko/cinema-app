{
    "version": 2,
    "builds": [
        {
        "src": "./server/app.js",
        "use": "@vercel/node"
        },
        {
        "src": "./client/build",
        "use": "@vercel/static"
        }
    ],
    "routes": [{ "src": "/(.*)", "dest": "/server/app.js" }],
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/server/app.js"
        }
      ]
}